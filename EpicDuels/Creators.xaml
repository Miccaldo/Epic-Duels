<Window x:Class="EpicDuels.Creators"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpicDuels"
        mc:Ignorable="d"
        Title="Creators"
         Height="768" 
        Width="1366"
        WindowStyle="None">

    <Grid x:Name="menu">
        <Grid.Background>
            <ImageBrush ImageSource="Images/background/mainWindow.jpg" Stretch="UniformToFill"/>
        </Grid.Background>

        <Label 
            Content="Autorzy: Michal Janowski" 
            HorizontalAlignment="Center" 
            Margin="0,60,0,0" 
            VerticalAlignment="Center" 
            Height="90" 
            Width="697" 
            FontSize="56" 
            FontFamily="Neverwinter" Foreground="Black"/>

        <Button x:Name="Return" 
                Content="Wstecz" 
                 HorizontalAlignment="Center" 
                Margin="0,0,300,150" 
                VerticalAlignment="Bottom"
                Width="300" 
                Height="100" 
                Foreground="White" 
                FontSize="62" 
                FontFamily="Neverwinter"
                Click="ReturnButton_Click"
                Background="{x:Null}"/>

    </Grid>

</Window>
